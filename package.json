{
  "name": "@grubersjoe/slide-menu",
  "version": "1.1.0",
  "author": "Jonathan Gruber <gruberjonathan@gmail.com>",
  "license": "MIT",
  "description": "A library agnostic multilevel JavaScript menu with a smooth slide effect",
  "repository": {
    "type": "git",
    "url": "https://github.com/grubersjoe/slide-menu.git"
  },
  "bugs": "https://github.com/grubersjoe/slide-menu/issues",
  "main": "dist/slide-menu.js",
  "scripts": {
    "build": "npm run clean && webpack --mode production && npm run post-build",
    "build:dev": "npm run clean && webpack --mode development && npm run post-build",
    "clean": "rm -f ./dist/*",
    "post-build": "rm -f dist/demo.js*",
    "start": "webpack-dev-server --mode production --host 0.0.0.0",
    "start:dev": "webpack-dev-server --mode development --host 0.0.0.0"
  },
  "keywords": [
    "menu",
    "multi level",
    "navigation"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "autoprefixer": "^9.0.0",
    "babel-loader": "^8.0.2",
    "core-js": "^2.5.7",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.0",
    "custom-event-polyfill": "^1.0.6",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.0.1",
    "ts-loader": "^5.0.0",
    "tslint": "^5.10.0",
    "typescript": "^3.0.0",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.0.0",
    "webpack-dev-server": "^3.1.4"
  }
}
